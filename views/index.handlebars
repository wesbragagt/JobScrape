<ul class="collection with-header">
    <li class="collection-header">
        <h4>JobScrape</h4>
        <a class="btn waves-effect waves-light" href="/scrape" name="action">Scrape
            <i class="material-icons right"></i>
        </a>
        <a class="btn waves-effect red" href="/deleteAll" name="action">Delete All
            <i class="material-icons right"></i>
        </a>
    </li>
    {{#each jobs}}
    <li class="collection-item">{{this.title}}</li>
    <li class="collection-item"><a target="_blank" href="{{this.link}}">{{this.link}}</a></li>
    {{/each}}


</ul>